<style>
  header {
    background-color: #EBBB99;
    display: flex;
    justify-content: space-between; 
    align-items: center;
    padding: 0.5rem 2rem;
    font-family: 'Poppins', sans-serif;
    font-weight: bold; 
    font-size: 0.7rem;
  }

  .center {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    gap: 2rem;
    font-family: "Poppins", sans-serif;
    font-size: 0.7rem;
    font-weight: bold;
  }

  .left, .right {
    display: flex;
    align-items: center;
  }

  a {
    text-decoration: none;
    color: black;
    cursor: pointer;
  }

  a:hover {
    text-decoration: underline;
  }

  .spacer {
    flex: 1;
  }

  .social-icons {
  display: flex;
  gap: 1rem;
}

.social-icons img {
  width: 16px;
  height: 16px;
  display: block;
  filter: brightness(0);
}


.social-icons img:hover {
  opacity: 0.6;
  transition: 0.2s ease;
}

.center svg,
.right svg {
  width: 12px;       
  height: 12px;
  margin-right: 0.3rem; 
  vertical-align: middle; 
}


</style>

